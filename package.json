{
  "name": "htsl-component",
  "version": "0.1.4",
  "description": "htsl React-like Component Class",
  "main": "dist/bundles/index.js",
  "module": "dist/bundles/index.mjs",
  "jsnext:main": "dist/bundles/index.mjs",
  "scripts": {
    "prebuild": "eslint src test",
    "build": "npm run bundles-build",
    "bundles-build": "rollup -c",
    "bundles-watch": "rollup -c -w",
    "pretest": "npm run build",
    "test": "mocha",
    "karma": "karma start",
    "cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha test/*.js",
    "release":"standard-version",
    "prepublish": "npm test"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "keywords": [
    "component",
    "react-like",
    "view",
    "html"
  ],
  "author": "Gilles Coomans",
  "license": "MIT",
  "dependencies": {
    "async-aggregator": "^0.0.4"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "babel-cli": "^6.14.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-unassert": "^2.1.2",
    "babel-preset-es2015": "^6.14.0",
    "babel-register": "^6.14.0",
    "babelrc-rollup": "^3.0.0",
    "chai": "^3.5.0",
    "coveralls": "^2.12.0",
    "cross-env": "^4.0.0",
    "eslint": "^3.15.0",
    "eslint-plugin-import": "^2.2.0",
    "karma": "^1.4.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-rollup-preprocessor": "^4.0.0",
    "karma-safari-launcher": "^1.0.0",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-watch": "^3.2.2",
    "standard-version": "^4.0.0",
    "uglify-js": "^2.7.5"
  }
}
